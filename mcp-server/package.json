{
  "name": "@cryptek/non-speculative",
  "version": "0.0.0",
  "description": "Documentation-focused MCP server with RAG and semantic search",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cryptekbits/Non-Speculative.git",
    "directory": "mcp-server"
  },
  "bugs": {
    "url": "https://github.com/cryptekbits/Non-Speculative/issues"
  },
  "homepage": "https://github.com/cryptekbits/Non-Speculative#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc --watch",
    "lint": "eslint .",
    "pretest": "npm run build",
    "test": "vitest --run",
    "test:unit": "vitest --run -c vitest.config.ts",
    "test:integration": "vitest --run -c vitest.integration.config.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm test",
    "start": "node dist/index.js --http --port 8080"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@zilliz/milvus2-sdk-node": "^2.6.2",
    "@ai-sdk/groq": "^2.0.25",
    "ai": "^6.0.0-beta.81",
    "chokidar": "^4.0.3",
    "ws": "^8.18.3",
    "lru-cache": "^11.2.2",
    "ignore": "^5.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.9.3",
    "vitest": "^2.1.4",
    "dotenv": "^16.4.5"
  }
}

